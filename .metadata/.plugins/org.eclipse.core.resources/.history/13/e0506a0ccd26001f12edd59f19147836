
public class DisplayCarsTrains extends MyFrame{
	public void run() {
		Car[] cars=new Car[2];
		cars[0]=new Car(10,30,3,0);
		cars[1]=new Car(10,100,5,0);
		for(int i=0; i<30; i++) {
			clear();
			for(int j=0; j<cars.length; j++) {
				cars[j].draw(this);
				cars[j].move();
			}
			sleep(0.1);
		}
		
	}

}
