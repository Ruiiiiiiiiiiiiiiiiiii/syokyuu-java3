import java.util.Vector;
public class DisplayFaceVector extends MyFrame {
	public void run() {
		Vector<Face> faces=new Vector<Face>();
		
		faces.add(new Face(30,50,20,1));
		faces.add(new Face(30,100,20,2));
		faces.add(new Face(30,150,20,3));
		faces.add(new Face(30,200,20,4));
		faces.add(new Face(30,250,20,5));
		
		for(int i=0; i<5; i++) {
			
			clear();
			
			for(int j=0; j<faces.size();j++) {
				faces.get(j).draw(this);
				faces.get(j).move();
				
			}
			sleep(0.1);
			
		}
	}

}
