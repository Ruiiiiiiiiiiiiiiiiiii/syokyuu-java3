import java.util.Vector;
public class DisplayFaceVector extends MyFrame {
	public void run() {
		Vector<Face> faces=new Vector<Face>();
		
		faces.add(new Face(30,50,10,1));
		faces.add(new Face(30,100,20,2));
		faces.add(new Face(30,150,30,3));
		faces.add(new Face(30,200,40,4));
		faces.add(new Face(30,250,50,5));
		
		for(int i=0; i<=5; i++) {
			
			clear();
			
			for(int j=0; j<faces.size();j++) {
				faces.get(j).draw(this);
			}
				faces.get(i).move();
				
			
			sleep(0.1);
			
		}
	}

}
