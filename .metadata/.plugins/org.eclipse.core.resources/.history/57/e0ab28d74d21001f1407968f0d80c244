
public class Face {
	int x,y, vx,vy;
	public Face(int x,int y,int vx, int vy) {
		this.x=x;
		this.y=y;
		this.vx=vx;
		this.vy=vy;
	}
	public void draw(MyFrame frame) {
		frame.fillRect(x+10,y+50,45,16);
		frame.fillRect(x,y+60,72,15);
		frame.fillOval(x+5,y+70,20,20);
		frame.fillOval(x+25,y+70,20,20);
	}
	public void move() {
		x += vx ;
		
	}

}
